--langdef=pyret
--langmap=pyret:.arr

--regex-pyret=/^[ \t]*fun[ \t]+([a-zA-Z0-9_-]+)/\1/f,function/
--regex-pyret=/^[ \t]*method[ \t]+([a-zA-Z0-9_-]+)/\1/m,method/
--regex-pyret=/^[ \t]*data[ \t]+([a-zA-Z0-9_-]+)/\1/d,data/
--regex-pyret=/^[ \t]*\|[ \t]*([a-zA-Z0-9_-]+)\([^)]*\)[ \t]*$/\1/d,data/
--regex-pyret=/^[ \t]*type[ \t]+([a-zA-Z0-9_-]+)/\1/d,data/
